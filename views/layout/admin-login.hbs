<style type="text/css">

.alert {
    padding: 40px;
    margin-bottom: 20px;
    border: 1px solid transparent;
    border-radius: 4px;
}
  
.alert-success {
color: #FFF;
background-color: #19B698;
border-color: #0FAC8E;
}
  
.alert-info {
color: #FFF;
background-color: #4AA3DF;
border-color: #4099D5;
}
.notice_wrapper {
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 999;
    background: rgba(135,216,211,0.5);
   
}
.notice_wrapper_error {
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 999;
    background: rgba(102, 4, 4, 0.5);
   
}
.alert, .notice {
    padding-top: 1.7rem;
    text-align: center;
    margin: 0;
    font-size: 1.25rem;
    font-weight: 700;
    color: white;
    letter-spacing: 1px;
}

.alert-danger {
    color: #FFF;
    background-color: #EA6153;
    border-color: #E05749;
}

</style>

